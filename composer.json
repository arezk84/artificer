{
    "name": "mascame/artificer",
    "description": "Artificer is an admin package built on top of your Eloquent models.",
    "homepage": "https://github.com/marcmascarell/laravel-artificer",
    "keywords": [
        "laravel",
        "admin",
        "artificer",
        "admin interface",
        "eloquent admin",
        "administration"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Mascarell",
            "email": "marcmascarell@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.*",
        "mascame/notify": "0.1.*",
        "mascame/arrayer": "1.*",
        "mcamara/laravel-localization": "0.15.*"
    },
    "require-dev": {
        "sami/sami": "2.0.0"
    },
    "autoload": {
        "classmap": [
            "src/commands",
            "src/controllers"
        ],
        "psr-0": {
            "Mascame\\Artificer\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
